{
	"priority": 10,
	"request": {
		"method": "GET",
		"urlPathPattern": "/file/.+"
	},
	"response": {
		"transformers": ["response-template"],
		"status": 200,
		"bodyFileName": "{{request.requestLine.pathSegments.[1]}}.xml",
		"headers": {
			"Content-Type": "application/xml"
		}
	},
	"metadata":{
		"comments":"introducing handlebars into filename"
	}
}